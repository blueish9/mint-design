{
  "name": "mint-design",
  "version": "1.0.0",
  "main": "lib/module/index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "prepare": "bob build"
  },
  "dependencies": {
    "expo": "~45.0.0",
    "expo-status-bar": "~1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@react-native-community/eslint-config": "^3.0.3",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
    "eslint": "^8.20.0",
    "prettier": "^2.7.1",
    "react-native-builder-bob": "^0.18.3",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-dom": "17.0.2",
    "react-native-web": "0.17.7",
    "typescript": "~4.3.5"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "private": true,
  "react-native": "src/index.tsx",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
