{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "target": "es6",
    "module": "commonjs",
    "lib": ["esnext", "es2017"],
    "jsx": "react",
    "sourceMap": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules",
    "jest",
  ]
}
